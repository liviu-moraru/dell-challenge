@model IEnumerable<DellChallenge.D2.Web.Models.ProductModel>

@{
    ViewData["Title"] = "View";
}
<h4>All Products</h4>
<hr />

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-md-4">
            <a asp-action="Edit" asp-route-id="@item.Id" >@item.Name - @item.Category</a>
        </div>
    </div>
}